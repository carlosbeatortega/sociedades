<h1>Cabeceras list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Usuarios_id</th>
            <th>Nombre</th>
            <th>Id_usuarios</th>
            <th>Campo</th>
            <th>Orden</th>
            <th>Titulo</th>
            <th>Condicion</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('cabeceras_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.usuariosid }}</td>
            <td>{{ entity.nombre }}</td>
            <td>{{ entity.idusuarios }}</td>
            <td>{{ entity.campo }}</td>
            <td>{{ entity.orden }}</td>
            <td>{{ entity.titulo }}</td>
            <td>{{ entity.condicion }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('cabeceras_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('cabeceras_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('cabeceras_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
