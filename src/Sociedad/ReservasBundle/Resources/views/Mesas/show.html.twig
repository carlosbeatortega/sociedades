<h1>Mesas</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Sociedades_id</th>
            <td>{{ entity.sociedadesid }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.nombre }}</td>
        </tr>
        <tr>
            <th>Planta</th>
            <td>{{ entity.planta }}</td>
        </tr>
        <tr>
            <th>Sala</th>
            <td>{{ entity.sala }}</td>
        </tr>
        <tr>
            <th>Comensales</th>
            <td>{{ entity.comensales }}</td>
        </tr>
        <tr>
            <th>Foto</th>
            <td>{{ entity.foto }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('mesas') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('mesas_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('mesas_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
