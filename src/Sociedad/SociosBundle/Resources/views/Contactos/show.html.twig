<h1>Contactos</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Sociedades_id</th>
            <td>{{ entity.sociedadesid }}</td>
        </tr>
        <tr>
            <th>Socios_id</th>
            <td>{{ entity.sociosid }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.nombre }}</td>
        </tr>
        <tr>
            <th>Fijo</th>
            <td>{{ entity.fijo }}</td>
        </tr>
        <tr>
            <th>Movil</th>
            <td>{{ entity.movil }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('contactos') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('contactos_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('contactos_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
