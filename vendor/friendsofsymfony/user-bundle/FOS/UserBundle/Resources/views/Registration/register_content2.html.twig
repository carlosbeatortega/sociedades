<form class="well form-search modal" action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
      {{ form_errors(form) }}
      <h2 class="hidden-phone hidden-tablet"> {{ 'alta.usuario'|trans }} </h2>
      <h5 class="hidden-desktop"> {{ 'alta.usuario'|trans }} </h5>
      <div class="container-fluid">
          <div class="row-fluid">
            <div class="span8">
                <table class="sin_relleno">
                 <tr>
                   <div class="control-group">
                    <td class="hidden-phone hidden-tablet"><strong>{{ form_label(form.username) }}</strong></td>
                    <div class="controls">
                        <td>{{ form_widget(form.username, { 'attr': {'class': 'span8','placeholder':"form.username"|trans({}, "FOSUserBundle") } }) }}{{ form_errors(form.username) }}</td>
                    </div>
                   </div>
                 </tr>
                 <tr>
                   <div class="control-group">
                    <td class="hidden-phone hidden-tablet"><strong>{{ form_label(form.email) }}</strong></td>
                    <div class="controls">
                        <td>{{ form_widget(form.email, { 'attr': {'class': 'span8','placeholder':"form.email"|trans({}, "FOSUserBundle") } }) }}{{ form_errors(form.email) }}</td>
                    </div>
                   </div>
                 </tr>
                 <tr>
                   <div class="control-group">
                    <td class="hidden-phone hidden-tablet"><strong>{{ form_label(form.plainPassword) }}</strong></td>
                    <div class="controls">
                        <td>{{ form_widget(form.plainPassword, { 'attr': {'class': 'span8','placeholder':"form.plainPassword"|trans({}, "FOSUserBundle") } }) }}{{ form_errors(form.plainPassword) }}</td>
                    </div>
                   </div>
                 </tr>
                 <tr>
                   <div class="control-group">
                    <td class="hidden-phone hidden-tablet"><strong>{{ form_label(form.name) }}</strong></td>
                    <div class="controls">
                        <td>{{ form_widget(form.name, { 'attr': {'class': 'span8','placeholder':"form.name"|trans({}, "FOSUserBundle") } }) }}{{ form_errors(form.name) }}</td>
                    </div>
                   </div>
                 </tr>
                 <tr>
                   <div class="control-group">
                    <td class="hidden-phone hidden-tablet"><strong>{{ form_label(form.foto) }}</strong></td>
                    <div class="controls">
                        <td>{{ form_widget(form.foto, { 'attr': {'class': 'span8','placeholder':"form.foto"|trans({}, "FOSUserBundle") } }) }}{{ form_errors(form.foto) }}</td>
                    </div>
                   </div>
                 </tr>
                 <tr>
                   <div class="control-group">
                    <td><strong>{{ form_label(form.dni) }}</strong></td>
                    <div class="controls">
                        <td>{{ form_widget(form.dni, { 'attr': {'class': 'span8','placeholder':"form.dni"|trans({}, "FOSUserBundle") } }) }}{{ form_errors(form.dni) }}</td>
                    </div>
                   </div>
                 </tr>
                 <tr>
                   <div class="control-group">
                    <td class="hidden-phone hidden-tablet"><strong>{{ form_label(form.fechanacimiento) }}</strong></td>
                    <div class="controls">
                        <td>{{ form_widget(form.fechanacimiento, { 'attr': {'class': 'span8','placeholder':"form.fechanacimiento"|trans({}, "FOSUserBundle") } }) }}{{ form_errors(form.fechanacimiento) }}</td>
                    </div>
                   </div>
                 </tr>
                 <tr>
                   <div class="control-group">
                    <td class="hidden-phone hidden-tablet"><strong>{{ form_label(form.numero_cuenta) }}</strong></td>
                    <div class="controls">
                        <td>{{ form_widget(form.numero_cuenta, { 'attr': {'class': 'span8','placeholder':"form.numero_cuenta"|trans({}, "FOSUserBundle") } }) }}{{ form_errors(form.numero_cuenta) }}</td>
                    </div>
                   </div>
                 </tr>
               </table>
               {{ form_rest(form) }}
            </div>
          </div>
      </div>
    <div>
        <input class="btn" type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
</form>
